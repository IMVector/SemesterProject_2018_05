<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.vector.pojo.BackpackSetting" table="BackpackSetting" schema="dbo" catalog="InstalHMS" optimistic-lock="version">
        <id name="backpackSettingId" type="int">
            <column name="BackpackSettingId" />
            <generator class="native" />
        </id>
        <property name="backpackSqlFile" type="string">
            <column name="BackpackSqlFile" not-null="true" />
        </property>
        <property name="backpackBatFile" type="string">
            <column name="BackpackBatFile" not-null="true" />
        </property>
        <property name="backpackToPath" type="string">
            <column name="BackpackToPath" not-null="true" />
        </property>
    </class>
</hibernate-mapping>
